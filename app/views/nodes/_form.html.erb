<%= simple_form_for(@node) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :longitude %>
    <%= f.input :latitude %>
    <%= f.input :title %>
    <%= f.input :resume %>
    <%= f.input :begin_at%>
    <%= f.input :end_at%>
    <%= f.association :topic %>
    <%= f.association :characters %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
